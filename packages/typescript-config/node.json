{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Node",
    "extends": "./base.json",
    "compilerOptions": {
        "lib": [
            "ES2021"
        ],
        "module": "ES2015",
        "target": "ES2021",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    }
}